<% include headerAndSide.ejs %>
<!DOCTYPE html>
<html>
<head>
    <title>Contact form using Bootstrap 3.3.4</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta charset="utf-8">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/sb-admin.css">

    
</head>
<body style="background-color: #7e7e7e">
	<div class="content-wrapper">

      <div class="container-fluid">

        <!-- Breadcrumbs -->
        <ol class="breadcrumb">
          <li class="breadcrumb-item">
            <a href="/">Dashboard</a>
          </li>
	  <li class="breadcrumb-item">
            <a href="/coattain">CO Attainment</a>
          </li>
          
        </ol>

        <div class="col-sm-12 col-sm-offset-3">



    <div class="well" style="margin-top: 5%;">
    <h3>CO Attainment</h3>
    <form role="form" id="contactForm123" data-toggle="validator" class="shake" method="post" action="/virtualPage7">
       

  <div class="form-group">
  <label for="sel1">Select list:</label>
  <select class="form-control" id="sel1" name="method">
    <option name="test1">test1</option>
    <option name="lab19">lab19</option>
    <option name="theory">theory</option>
    <option name="oral">oral</option>
  </select>
</div>
   

	<div class="form-group">
	   <br>
	   Weightage
	   <input type="text" class="form-control" name="weightage" placeholder="weightage">
	</div>

	<div class="form-group">
	   <br>
	   Total marks
	   <input type="text" class="form-control" name="totalmarks" placeholder="totalmarks">
	</div>

	<div class="form-group">
	   <br>
	    Minimum Marks
	   <input type="text" class="form-control" name="minmarks" placeholder="minmarks">
	</div>

	<div class="form-group">
	    <br>
	    Number of Students
	   <input type="text" class="form-control" name="numstu" placeholder="numstu">
	</div>

	
	<br>
        <button name="submit" type="submit" id="form-submit" class="btn btn-success btn-md pull-right ">Submit</button>
        <div id="msgSubmit" class="h3 text-center hidden"></div>
        <div class="clearfix"></div>
    </form>
    </div>
     <hr>
     <br><br>

	


    <!-- Displaying Data from Database Objectives-->

<div >
  <h3>CO Attainment</h3>          
  <table class="table table-striped table-hover table-bordered">
    <thead>
      <tr>
        <th>Method</th>
        <th>Weightage</th>
        <th>Total marks</th>
        <th>Minimum Marks</th>
        <th>Number of Students</th>
        <th>Attainment %</th>
        <th>Attainment Level</th>
        
      </tr>
    </thead>
    <tbody>
    	<% obj.forEach(function(row,index){%>

      <tr>
        <td> <%= row.method %>     </td>
        <td> <%= row.weightage %>     </td>
        <td> <%= row.totalmarks %>	</td>
	      <td> <%= row.minmarks %>         </td>
	     <td> <%= row.numstu %> 	</td>
	     <td> <%= row.attainper %>   </td>
        <td> <%= row.attainlevel %>   </td>
      </tr>
      <% }); %>
    </tbody>
  </table>

</div>

</body>
</html>