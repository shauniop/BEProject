<% include headerAndSide.ejs %>
<!DOCTYPE html>
<html>

<head>
    <title>Contact form using Bootstrap 3.3.4</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta charset="utf-8">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    
    
</head>


<body style="background-color: #7e7e7e">

	<div class="content-wrapper">
      <div class="container">

          <!-- Breadcrumbs -->
          <ol class="breadcrumb">
              <li class="breadcrumb-item">
                <a href="/">Dashboard</a>
              </li>
              <li class="breadcrumb-item">
                  <a href="/report">Report generation</a>
              </li>
          </ol>



         <ul class="nav nav-tabs" role = "tablist">
            <li class="nav-item">
              <a class="nav-link active" href="#part1" role="tab" data-toggle="tab">Generate CO Report</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="#part2" role="tab" data-toggle="tab">Generate PO Report</a>
            </li>
            
        </ul>



        <div class="tab-content">
              <!--<h3>Add Course</h3>-->
              <div class="tab-pane active" role="tabpanel" id="part1">
                  <form role="form" data-toggle="validator" class="shake" method="post" action="/COReport">
                     
                      <div class="form-group">
                         <br>
                            Year
                         <input type="text" class="form-control" name="year" >
                      </div>
                      

                      <div class="form-group">
                         <br>
                             Course Name
                         <input type="text" class="form-control" name="courseName" placeholder="Course Name">
                      </div>

                      
                      <br>
                            <button name="submit" type="submit" id="form-submit" class="btn btn-success btn-md pull-right ">Submit</button>
                            <div id="msgSubmit" class="h3 text-center hidden"></div>
                            <div class="clearfix"></div>
                        </form>
                  </form>
              </div>




              <!--<h3>Add Course Outcome</h3>-->
              <br>
              <div class="tab-pane " role="tabpanel" id="part2">
                  <form role="form" data-toggle="validator" class="shake" method="post" action="/CourseOutcome">
                     

                      

                      <div class="form-group">
                         <br>
                         Course Outcome ID
                         <input type="text" class="form-control" name="courseID" >
                      </div>

                      <div class="form-group">
                         <br>
                         Text
                         <input type="text" class="form-control" name="text" >
                      </div>

                      <div class="form-group">
                         <br>
                         Year
                         <input type="text" class="form-control" name="year" >
                      </div>

                     <div class="form-group">
                         <br>
                            Indirect attain
                         <input type="text" class="form-control" name="indirectAttain" >
                      </div>
                      
                      
                      <br>
                            <button name="submit" type="submit" id="form-submit" class="btn btn-success btn-md pull-right ">Submit</button>
                            <div id="msgSubmit" class="h3 text-center hidden"></div>
                            <div class="clearfix"></div>
                  </form>
                   <hr>
              </div>





              
       </div>
     </div>
  </div>


</body>
</html>