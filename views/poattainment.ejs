          <th>PO6</th>
          <th>PO7</th>
          <th>PO8</th>
          <th>PO9</th>
          <th>PO10</th>
          <th>PO11</th>
          <th>PO12</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          
          <td>
            <input type="text" class="form-control" size="16" name="po1" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po2" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po3" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po4" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po5" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po6" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po7" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po8" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po9" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po10" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po11" value="">
          </td>
          <td>
            <input type="text" class="form-control" size="16" name="po12" value="">
          </td>

        </tr>
      </tbody>
    </table>
  </div>


        <br>
        <button name="submit" type="submit" id="form-submit" class="btn btn-success btn-md pull-right ">Submit</button>
        <div id="msgSubmit" class="h3 text-center hidden"></div>
        <div class="clearfix"></div>
    </form>
    <hr>
    <br><br>
