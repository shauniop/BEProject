  <div class="tab-content">
     <div class="tab-pane active" role="tabpanel" id="POTable">
    <!--<h3>Make PO Table</h3>-->
    <form role="form"  data-toggle="validator" class="shake" method="post" action="/virtualPage10">
        
        <div class="form-group">
           <br>
           PO ID
           <input type="text" class="form-control" name="poID" >
        </div>

        <div class="form-group">
           <br>
           Text
           <input type="text" class="form-control" name="textpo" >
        </div>

        
        <br>
              <button name="submit" type="submit" id="form-submit" class="btn btn-success btn-md pull-right ">Submit</button>
              <div id="msgSubmit" class="h3 text-center hidden"></div>
              <div class="clearfix"></div>
          </form>
           <hr>
           <br><br>
    </div>



     <div class="tab-pane" role="tabpanel" id="POA">
     <!--<h3>PO Attainment</h3>-->
     <form role="form" id="contactForm" data-toggle="validator" class="shake" method="post" action="/virtualPage8">
            

			<div class="form-group">
			    <label for="sel1">Select Course Outcome</label>
			    <select class="form-control" id="sel1" name="courseID">
			     <% obj1.forEach(function(COrows,index){%>
			      <option name="<%= COrows.courseID %>"><%= COrows.courseID %></option>
			    <% }); %>
			    </select>
			  </div>


		

  <div class="row">
    <table class="table">
      <thead>
        <tr>
          <th>PO1</th>
          <th>PO2</th>
          <th>PO3</th>
