<% include headerAndSide.ejs %>
<!DOCTYPE html>
<html>
<head>
    <title>Contact form using Bootstrap 3.3.4</title>
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta charset="utf-8">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <link rel="stylesheet" href="/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/sb-admin.css">
</head>
<body style="background-color: #7e7e7e">
	<div class="content-wrapper">

      <div class="container-fluid">

        <!-- Breadcrumbs -->
        <ol class="breadcrumb">
          <li class="breadcrumb-item">
            <a href="/">Dashboard</a>
          </li>
	  <li class="breadcrumb-item">
            <a href="/plans.html">Plans</a>
          </li>
          <li class="breadcrumb-item active">Lab</li>
        </ol>

        <div class="col-sm-12 col-sm-offset-3">


    <div class="well" style="margin-top: 5%;">
    <h3>Plan</h3>
    <form role="form" id="contactForm123" data-toggle="validator" class="shake" method="post" action="/virtualPage4">
       
	<div class="form-group">
	   <br>
	   Expt No.
	   <input type="text" class="form-control" name="expt" placeholder="Enter Exp No.">
	</div>

	<div class="form-group">
	   <br>
	   Date A
	   <input type="date" class="form-control" name="dateA" placeholder="Enter Date A">
	</div>

	<div class="form-group">
	   <br>
	    Date B
	   <input type="date" class="form-control" name="dateB" placeholder="Enter Date B">
	</div>

	<div class="form-group">
	    <br>
	   Date C
	   <input type="date" class="form-control" name="dateC" placeholder="Enter Date C">
	</div>

	<div class="form-group">
	    <br>
	    Date D
	   <input type="date" class="form-control" name="dateD" placeholder="Enter Date D">
	</div>
	
        <div class="form-group">
           <br>
	    Concept
            <textarea id="message123" class="form-control" rows="2" placeholder="Enter the Concept" name="concept" required ></textarea>
            <div class="help-block with-errors"></div>
        </div>

	 <div class="form-group">
           <br>
	    Title/Aim
            <textarea id="message456" class="form-control" rows="2" placeholder="Enter the Title/Aim" name="aim" required ></textarea>
            <div class="help-block with-errors"></div>
        </div>	
	
	<br>
        <button name="submit" type="submit" id="form-submit" class="btn btn-success btn-md pull-right ">Submit</button>
        <div id="msgSubmit" class="h3 text-center hidden"></div>
        <div class="clearfix"></div>
    </form>
    </div>
    <hr>
    <br><br>

<div >
  <h2>Labs</h2>        
  <table class="table table-striped table-hover table-bordered">
    <thead>
      <tr>
	<th>Expt no.</th>
	<th>Date A</th>
        <th>Date B</th>
        <th>Date C</th>
        <th>Date D</th>
	<th>Concept</th>
	<th>Aim</th>
        
      </tr>
    </thead> 
    <tbody>
    	<% obj.forEach(function(row,index){%>
      <tr>
	<td><%= row.Expt %> </td>
	<td><%= row.DateA %> </td>
        <td><%= row.DateB %> </td>
	<td><%= row.DateC %> </td>
	<td><%= row.DateD %> </td>
        <td><%= row.Concept %> </td>
	<td><%= row.Aim %> </td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>



</body>
</html>



